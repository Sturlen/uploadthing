{
  "name": "uploadthing",
  "type": "module",
  "version": "6.1.1",
  "engines": {
    "node": ">=18.13.0"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx}\" --max-warnings 0",
    "build": "rm -rf dist && bunchee",
    "clean": "git clean -xdf dist node_modules",
    "dev": "bunchee -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@uploadthing/mime-types": "^0.2.2",
    "@uploadthing/shared": "^6.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "18.2.33",
    "@uploadthing/eslint-config": "0.2.0",
    "@uploadthing/tsconfig": "0.1.0",
    "bunchee": "^4.3.3",
    "eslint": "^8.47.0",
    "express": "^4.18.2",
    "fastify": "^4.23.2",
    "h3": "^1.8.1",
    "next": "14.0.1",
    "solid-js": "^1.7.11",
    "tailwindcss": "^3.3.2",
    "type-fest": "^3.11.1",
    "typescript": "^5.3.3",
    "undici": "^5.20.0",
    "vitest": "^0.30.1",
    "zod": "^3.22.3"
  },
  "peerDependencies": {
    "express": "*",
    "fastify": "*",
    "h3": "*",
    "next": "*",
    "tailwindcss": "*",
    "zod": "*"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "h3": {
      "optional": true
    },
    "next": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@uploadthing/eslint-config/base"
    ]
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./client": {
      "import": {
        "types": "./dist/client.d.mts",
        "default": "./dist/client.mjs"
      },
      "require": {
        "types": "./dist/client.d.ts",
        "default": "./dist/client.js"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server.d.mts",
        "default": "./dist/server.mjs"
      },
      "require": {
        "types": "./dist/server.d.ts",
        "default": "./dist/server.js"
      }
    },
    "./next": {
      "import": {
        "types": "./dist/next.d.mts",
        "default": "./dist/next.mjs"
      },
      "require": {
        "types": "./dist/next.d.ts",
        "default": "./dist/next.js"
      }
    },
    "./next-legacy": {
      "import": {
        "types": "./dist/next-legacy.d.mts",
        "default": "./dist/next-legacy.mjs"
      },
      "require": {
        "types": "./dist/next-legacy.d.ts",
        "default": "./dist/next-legacy.js"
      }
    },
    "./express": {
      "import": {
        "types": "./dist/express.d.mts",
        "default": "./dist/express.mjs"
      },
      "require": {
        "types": "./dist/express.d.ts",
        "default": "./dist/express.js"
      }
    },
    "./tw": {
      "import": {
        "types": "./dist/tw.d.mts",
        "default": "./dist/tw.mjs"
      },
      "require": {
        "types": "./dist/tw.d.ts",
        "default": "./dist/tw.js"
      }
    },
    "./fastify": {
      "import": {
        "types": "./dist/fastify.d.mts",
        "default": "./dist/fastify.mjs"
      },
      "require": {
        "types": "./dist/fastify.d.ts",
        "default": "./dist/fastify.js"
      }
    },
    "./h3": {
      "import": {
        "types": "./dist/h3.d.mts",
        "default": "./dist/h3.mjs"
      },
      "require": {
        "types": "./dist/h3.d.ts",
        "default": "./dist/h3.js"
      }
    },
    "./internal/types": {
      "import": {
        "types": "./dist/internal/types.d.mts",
        "default": "./dist/internal/types.mjs"
      },
      "require": {
        "types": "./dist/internal/types.d.ts",
        "default": "./dist/internal/types.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts",
        "dist/*.d.mts"
      ]
    }
  }
}
